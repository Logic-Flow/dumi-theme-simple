{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": false,
    "declaration": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "jsx": "react",
    "skipLibCheck": true,
    "baseUrl": "./",
    "noImplicitAny": false,
    "paths": {
      "@@/*": ["example/.dumi/tmp/*"]
    }
  },
  "include": ["src/**/*"]
}
